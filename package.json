    "@changesets/cli": "^2.27.1",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  },
  "author": "Ali Khalouf"
}
